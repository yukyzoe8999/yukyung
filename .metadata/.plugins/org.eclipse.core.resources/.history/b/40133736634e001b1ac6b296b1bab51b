package Java_0104;

import java.util.Scanner;

public class CCC {
	BBB b = new BBB();
	
	public void scan(BBB b) {
		Scanner sc = new Scanner(System.in);
		System.out.println("네자리 숫자 입력");
		b.str = sc.nextLine();
		System.out.println(b.str);
		
		System.out.println("한자리 숫자 입력");
		b.a = sc.nextInt();
	

	}

}
