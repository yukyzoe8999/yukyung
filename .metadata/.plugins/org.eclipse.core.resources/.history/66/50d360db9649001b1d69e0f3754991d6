package Java_1229;

public class Exam {
	private int kor;
	private int eng;
	private int math;
	private int soci;
	private int scin;
	
	public int getSoci() {
		return soci;
	}
	public void setSoci(int soci) {
		this.soci = soci;
	}
	public int getScin() {
		return scin;
	}
	public void setScin(int scin) {
		this.scin = scin;
	}
	public int getKor() {
		return kor;
	}
	public void setKor(int kor) {
		this.kor = kor;
	}
	public int getEng() {
		return eng;
	}
	public void setEng(int eng) {
		this.eng = eng;
	}
	public int getMath() {
		return math;
	}
	public void setMath(int math) {
		this.math = math/2;
	}
}
