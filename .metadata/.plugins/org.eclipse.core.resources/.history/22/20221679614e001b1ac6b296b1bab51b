package Java_0104;

public class AAA {

	public static void main(String[] args) {
		CCC c = new CCC();
		BBB b = new BBB();
		String s[] = new String[10];

		c.scan(b);
	}

	public void print(CCC c) {
		for (int i = 0; i < 4; i++) {
			String st = c.b.str.substring(i, i + 1);
			System.out.println(st);
		}

	}
}
