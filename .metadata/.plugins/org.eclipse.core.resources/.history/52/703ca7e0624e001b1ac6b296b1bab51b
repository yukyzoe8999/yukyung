package Java_0104;

public class AAA {

	public static void main(String[] args) {
		CCC c = new CCC();
		BBB b = new BBB();
		//String s[] = new String[10];

	System.out.println(b.str);
		c.scan(b);
		print(c);
	}

	public static void print(CCC c) {
		BBB b = new BBB();	
		String s[] = new String[10];
		for (int i = 0; i < b.str.length(); i++) {
			s[i] = b.str.substring(i, i + 1);
			System.out.println(s[i]);
		}
		

	}
}
