package Java_1229;

import java.util.Scanner;

public class USER {
	String ID;
	String NAME;
	String PASS;
	
	public String getID() {
		System.out.println("ID:");
		return ID;
	}
	public void setID(String iD) {
		ID = iD;
	}
	public String getNAME() {
		return NAME;
	}
	public void setNAME(String nAME) {
		NAME = nAME;
	}
	public String getPASS() {
		return PASS;
	}
	public void setPASS(String string) {
		PASS = string;
	}
	


}
