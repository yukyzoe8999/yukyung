package Java_1229;

import java.util.Scanner;

public class USER {
	String ID;
	String NAME;
	int PASS;
	
	public String getID() {
		return ID;
	}
	public void setID(String iD) {
		ID = iD;
	}
	public String getNAME() {
		return NAME;
	}
	public void setNAME(String nAME) {
		NAME = nAME;
	}
	public int getPASS() {
		return PASS;
	}
	public void setPASS(int pASS) {
		PASS = pASS;
	}
	
	
	

}
